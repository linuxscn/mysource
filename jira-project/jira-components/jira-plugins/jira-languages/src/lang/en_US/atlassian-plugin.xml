<atlassian-plugin name="English (United States) Language Pack" key="${project.groupId}.${project.artifactId}.en_US"
                  plugins-version="2">
    <plugin-info>
        <description>This plugin contains translations the core keys in JIRA for English (United States).</description>
        <version>${pom.version}</version>
        <vendor name="Atlassian" url="http://www.atlassian.com/"/>
    </plugin-info>
    <language name="English (United States)" key="en_US" language="en" country="US"/>
    <resource name="i18n-0" type="i18n" location="com/atlassian/jira/web/action/"/>
</atlassian-plugin>
