<atlassian-plugin name="English (Upside Down) Language Pack" key="${project.groupId}.${project.artifactId}.en_UD"
                  plugins-version="2">
    <plugin-info>
        <description>This plugin contains the core keys in JIRA with upside down text.</description>
        <version>${pom.version}</version>
        <vendor name="Atlassian" url="http://www.atlassian.com/"/>
    </plugin-info>
    <language name="English (Upside Down)" key="en_UD" language="en" country="UD"/>
    <resource name="i18n-0" type="i18n" location="com/atlassian/jira/web/action/"/>
</atlassian-plugin>
